{
  "name": "wikiembed",
  "description": "wikipedia content box plugin",
  "version": "0.0.1",
  "license": "MIT",
  "author": "fariborz.emami1997@gmail.com",
  "homepage": "",
  "devDependencies": {
    "kill-port": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-run": "^1.1.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.49.0",
    "sirv-cli": "^0.4.4",
    "svelte-adapter": "https://github.com/babakhani/svelte-adapter"
  },
  "scripts": {
    "clear": "kill-port --port 8000,8001,8002,8003",
    "svelte:build": "rollup -c",
    "build": "npm-run-all -p svelte:build vue:build react:build iife:build",
    "dev": "rollup -c ./rollup.config.js  -w",
    "install-vue": "cd ./demo/vue; npm install; cd ../../",
    "vue:build": "rollup -c ./adapter/rollup.vue.js",
    "vue:dev": "rollup -c ./adapter/rollup.vue.js -w",
    "vue:serve": "cd ./demo/vue; npm run serve; cd ../../",
    "vue": "npm-run-all -p vue:dev vue:serve",
    "install-react": "cd ./demo/react-demo; npm install; cd ../../",
    "react:build": "rollup -c ./adapter/rollup.react.js",
    "react:dev": "rollup -c ./adapter/rollup.react.js -w",
    "react:serve": "cd ./demo/react-demo ; npm start & cd ../../",
    "react:aliases": "ln -f public/wikiembed-react.js ./demo/react-demo/src/wikiembed-react.js",
    "react": "npm-run-all react:build -p react:dev react:aliases react:serve",
    "iife:build": "rollup -c ./adapter/rollup.iife.js",
    "iife:dev": "rollup -c ./adapter/rollup.iife.js -w",
    "iife:serve": "sirv ./demo/js --single --dev --port 8001",
    "iife:aliases": "ln -f public/wikiembed-iife.js ./demo/js/wikiembed-iife.js ; ln -f public/wikiembed.css ./demo/js/wikiembed.css",
    "iife": "npm-run-all iife:build -p iife:dev iife:aliases iife:serve",
    "start": "sirv public --single --port 8000",
    "start:dev": "sirv public --single --dev --port 8000"
  }
}
